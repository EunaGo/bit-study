<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- sql 설정 : sql문, 반환 타입, 파라미터 설정 -->

<mapper
	namespace="com.openproject.mvc.mybatis.mapper.MemberMapper">

	<insert id="insertMember" parameterType="com.openproject.mvc.model.Member">

	INSERT INTO test.member (uid, upw, uname, uphoto ) VALUES (#{uid},#{upw},#{uname},#{uphoto})
	</insert>
</mapper>